# emacs
alias em='emacs -nw'
alias emg='emacs &'
alias emc='emacsclient'

# git
alias gs='git status'
alias ga='git add'
alias gc='git commit'
alias gpl='git pull'
alias gph='git push'

# tmux
alias tnew='tmux new -s'
alias tattach='tmux attach -t'
alias tsesskill='tmux kill-session -t'
alias tls='tmux ls'

# sshfs
alias sshfs_m='~/projects/dotfiles/scripts/sshfs_mnt.sh'
alias sshfs_u='~/projects/dotfiles/scripts/sshfs_umnt.sh'
